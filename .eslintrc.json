{
  "env": {
    "node": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended" , "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console" :"warn"
  },
  "plugins": ["@typescript-eslint"]
}
